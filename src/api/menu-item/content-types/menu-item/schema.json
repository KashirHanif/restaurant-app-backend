{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "singularName": "menu-item",
    "pluralName": "menu-items",
    "displayName": "MenuItem",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "soft drinks,",
        "sweets,",
        "fast food,"
      ],
      "required": true
    },
    "restaurant": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::restaurant.restaurant",
      "inversedBy": "menu_items"
    },
    "cart_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart-item.cart-item",
      "mappedBy": "menu_item"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "menu_item"
    },
    "time_for_preparation": {
      "type": "integer",
      "default": 30
    }
  }
}
